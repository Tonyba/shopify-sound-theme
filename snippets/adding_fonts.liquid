
{% liquid

    assign font500 = 'ibm-plex-sans-v19-latin-500.woff2' | asset_url
    assign font700 = 'ibm-plex-sans-v19-latin-700.woff2' | asset_url
    assign font300 = 'ibm-plex-sans-v19-latin-300.woff2' | asset_url
    
%}

<style>
    @font-face {
        font-display: swap; 
        font-family: 'IBM Plex Sans';
        font-style: normal;
        font-weight: 700;
        src: url({{font700}}) format("woff2");
    }
    
    @font-face {
        font-display: swap; 
        font-family: 'IBM Plex Sans';
        font-style: normal;
        font-weight: 500;
        src: url({{font500}}) format("woff2");
    }
    
    @font-face {
        font-display: swap; 
        font-family: 'IBM Plex Sans';
        font-style: normal;
        font-weight: 300;
        src: url({{font300}}) format("woff2");
    }
    /*extra weights for main font end*/

</style>
